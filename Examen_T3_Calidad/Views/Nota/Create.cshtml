@{ Layout = null;    var nota = (Nota)Model;}<div class="row">    <div class="col-6">        <form id="createNota" onsubmit="guardar(event)" action="/Nota/Create" enctype="multipart/form-data" method="post">            <div class="form-group">                <label>Titulo</label>                <input type="text" class="form-control" name="Titulo" placeholder="Titulo" value="@Model.Titulo" />                @Html.ValidationMessage("Titulo")            </div>            <div class="form-group">                <label>Contenido</label>                <textarea id="text" name="Contenido" rows="8" cols="60"></textarea>                @Html.ValidationMessage("Contenido")            </div>            <div class="form-group">                <label>Etiquetas</label>                @foreach (var item in ViewBag.Etiquetas)                {                    <div class="form-check form-check">                        <input name="etiqueta" class="form-check-input" type="checkbox" id="inlineCheckbox1" value="@item.Id">                        <label class="form-check-label" for="inlineCheckbox1">@item.Nombre</label>                    </div>                }                @Html.ValidationMessage("etiqueta")            </div>        </form>    </div></div>
