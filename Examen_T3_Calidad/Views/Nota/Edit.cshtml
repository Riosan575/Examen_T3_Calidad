@{ Layout = null;    var nota = (Nota)Model;}<div class="row">    <div class="col-6">        <form id="editNota" onsubmit="editar(event)" action="/Nota/Edit" enctype="multipart/form-data" method="post">            <input id="mtxtId" type="hidden" name="Id" />            <div class="form-group">                <label>Titulo</label>                <input id="mtxtTitulo" type="text" class="form-control" name="Titulo" placeholder="Titulo"/>                @Html.ValidationMessage("Titulo")            </div>            <div class="form-group">                <label>Contenido</label>                <textarea id="mtxtContenido" name="Contenido" rows="8" cols="60"></textarea>                @Html.ValidationMessage("Contenido")            </div>        </form>    </div></div>